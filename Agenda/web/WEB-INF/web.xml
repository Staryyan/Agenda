<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>LogInServlet</servlet-name>
        <servlet-class>Servlet.LogInServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangeEmailServlet</servlet-name>
        <servlet-class>Servlet.ChangeEmailServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangePasswordServlet</servlet-name>
        <servlet-class>Servlet.ChangePasswordServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangePhoneServlet</servlet-name>
        <servlet-class>Servlet.ChangePhoneServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteUserServlet</servlet-name>
        <servlet-class>Servlet.deleteUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>Servlet.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateMeetingServlet</servlet-name>
        <servlet-class>Servlet.CreateMeetingServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AllMeetingServlet</servlet-name>
        <servlet-class>Servlet.AllMeetingServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogInServlet</servlet-name>
        <url-pattern>/LogInCheck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangeEmailServlet</servlet-name>
        <url-pattern>/ChangeEmailCheck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangePasswordServlet</servlet-name>
        <url-pattern>/ChangePasswordCheck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangePhoneServlet</servlet-name>
        <url-pattern>/ChangePhoneCheck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteUserServlet</servlet-name>
        <url-pattern>/DeleteUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateMeetingServlet</servlet-name>
        <url-pattern>/CreateMeeting</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AllMeetingServlet</servlet-name>
        <url-pattern>/AllMeeting</url-pattern>
    </servlet-mapping>
</web-app>